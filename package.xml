<?xml version="1.0" encoding="UTF-8"?>
<tred_extension xmlns="http://ufal.mff.cuni.cz/pdt/pml/" install_size="58864">
  <head>
    <schema href="tred_extension_schema.xml"/>
  </head>
  <pkgname>pdt_st_data</pkgname>
  <repository href="http://ufal.mff.cuni.cz/~pajas/tred/extensions/"/>
  <title>Display st-data in the tectogrammatical trees</title>
  <version>0.6</version>
  <copyright year="2010">Pavel Straňák</copyright>
  <description>Allows to open PML "*.st" files and display the multiword
      expressions annotated therein as node groups in tectogrammatical trees. 
  </description>
  <icon>icons/icon.png</icon>
  <require>
    <tred min_version="1.4391"/>
    <extension name="pdt20" min_version="1.0" href="http://ufal.mff.cuni.cz/~pajas/tred/extensions/pdt20"/>
  </require>
</tred_extension>
